{
    "name": "G-7",
    "version": "0.1",
    "manifest_version": 3,
    "description": "Uses the chrome.declarativeNetRequest API to block requests.",
    "background": {
        "service_worker": "service_worker.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "externally_connectable": {
        "matches": ["<all_urls>"]
    },
    "permissions": [
        "declarativeNetRequest",
        "sidePanel",
        "webRequest",
        "tabs",
        "storage",
        "nativeMessaging",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_title": "Click here!"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B"
            }
        }
    },
    "icons": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    }
}
